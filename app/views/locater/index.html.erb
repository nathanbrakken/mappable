<script type="text/javascript">

	var map;
	function initialize() {
	  var myLatlng = new google.maps.LatLng(40.74394060479145, -74.03214454650879);
	  var mapOptions = {
	    zoom: 12,
	    center: myLatlng,
	    mapTypeId: google.maps.MapTypeId.ROADMAP, 
	  }
	  map = new google.maps.Map(document.getElementById("map-canvas"), mapOptions);

	  google.maps.event.addListener(map, 'click', function(event) {
	    placeMarker(event.latLng);
	    console.log(event.latLng)
	  });
	  // placeMarker({"40.74394060479145, -74.03214454650879"});
	  // placeMarker(new google.maps.LatLng(40.74394060479145, -74.03214454650879));
	  
	}
	//usage addPlaceMarkerFromCoordinates(40.76624155398039, -73.97163391113281)
	function addPlaceMarkerFromCoordinates(lat, longitutde) {
		placeMarker(new google.maps.LatLng(lat, longitutde))
	}

	function placeMarker(location) {
	  var marker = new google.maps.Marker({
	      position: location,
	      map: map,
	      title: location.jb + ', ' + location.kb
	      //
	  });
	  infoWindow(marker);
	}

	var infoWindow = function (marker) {
		// console.log(marker.title)
		info = new google.maps.InfoWindow({'content' : marker.title});
		google.maps.event.addListener(marker, 'click', function(e){
			info.open(map, marker)
		})
	}
	google.maps.event.addDomListener(window, 'load', initialize);
</script>
